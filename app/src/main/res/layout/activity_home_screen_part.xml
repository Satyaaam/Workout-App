<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home_screen_part"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        android:textAlignment="center">

        <TextView
            android:id="@+id/feed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:fontFamily="@font/staatliches"
            android:text="FEED"
            android:textColor="@color/black"
            android:textSize="35sp" />

        <ImageView
            android:id="@+id/profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="240dp"
            android:layout_toRightOf="@+id/feed"
            android:src="@drawable/user_regular"
            android:textAlignment="center"
            android:layout_marginTop="13dp"/>
        <ImageView
            android:id="@+id/notes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/note_sticky_solid"
            android:layout_toRightOf="@+id/profile"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="13dp"
            android:textAlignment="center"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/bell_solid"
            android:layout_toRightOf="@id/notes"
            android:background="@color/white"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="705dp">

        <TextView
            android:id="@+id/tv_feed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/staatliches"
            android:text="Feed"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:layout_marginLeft="5dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/tv_feed"
            android:fontFamily="@font/staatliches"
            android:text="plan"
            android:textSize="20sp"
            android:textColor="@color/black"
            android:layout_marginLeft="10dp"/>


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>